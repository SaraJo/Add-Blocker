{
  "name": "Add Blocker",
  "version": "1.2.3",
  "description": "get the annoying out of your inbox",
  "background_page": "background.html",
  "content_scripts": [ 
    	{
    	  "js": ["js/addblocker.js"],
    	  "matches": [  "http://*.google.com/*", "https://*.google.com/*" ],
    	  "run_at" : "document_end",
    	  "all_frames": true
      }],
  "permissions": [
    "http://*.google.com/",
        "https://*.google.com/",
    		"idle"
  ]
}
